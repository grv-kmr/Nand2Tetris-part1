    Nand(a = c[1],b = notselcd1,out = notc1selcd1);
    Nand(a = d[1], b = sel[0], out = notd1notsel0);
    Nand(a = notc1selcd1, b = notd1notsel0, out = cd1);

    Nand(a = sel[1],b = sel[1], out = notselabcd1);
    Nand(a = ab1, b= notselabcd1, out = notab1selabcd1);
    Nand(a = cd1, b = sel[1], out = notcd1notsel1);
    Nand(a= notab1selabcd1, b = notcd1notsel1, out =out[1]);

