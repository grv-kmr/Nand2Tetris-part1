/**
 * 4-way 16-bit multiplexor:
 * out = a if sel = 00
 *       b if sel = 01
 *       c if sel = 10
 *       d if sel = 11
 */
CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];
    
    PARTS:
    //bit0
    Nand(a= sel[0],b = sel[0], out = notselab0);
    Nand(a= a[0], b = notselab0, out = nota0selab0);
    Nand(a = b[0], b = sel[0], out =notb0notsel0);
    Nand(a= nota0selab0, b = notb0notsel0, out = ab0);

    Nand(a =sel[0], b = sel[0], out = notselcd0);
    Nand(a = c[0],b = notselcd0,out = notc0selcd0);
    Nand(a = d[0], b = sel[0], out = notd0notsel0);
    Nand(a = notc0selcd0, b = notd0notsel0, out = cd0);

    Nand(a = sel[1],b = sel[1], out = notselabcd0);
    Nand(a = ab0, b= notselabcd0, out = notab0selabcd0);
    Nand(a = cd0, b = sel[1], out = notcd0notsel1);
    Nand(a= notab0selabcd0, b = notcd0notsel1, out =out[0]);

    //bit1
    Nand(a= sel[0],b = sel[0], out = notselab1);
    Nand(a= a[1], b = notselab1, out = nota1selab1);
    Nand(a = b[1], b = sel[0], out =notb1notsel0);
    Nand(a= nota1selab1, b = notb1notsel0, out = ab1);

