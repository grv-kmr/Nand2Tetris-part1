/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand (a=sel,b=sel,out=notsel);
    Nand (a=a,b=notsel,out=notasel);
    Nand (a=sel,b=b,out=notselnotb);
    Nand (a=notasel,b=notselnotb,out=out);
    //// Replace this comment with your code.
}